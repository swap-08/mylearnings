<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="empDetails">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" name="employee">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Ecode" type="xs:unsignedShort" />
              <xs:element name="Ename">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Fname" type="xs:string" />
                    <xs:element name="Lname" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Doj" type="xs:string" />
              <xs:element name="TotalExp" type="xs:unsignedByte" />
              <xs:element name="Department">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Unit" type="xs:string" />
                    <xs:element name="loc" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Designation">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="one" />
                    <xs:element name="two" type="xs:string" />
                    <xs:element name="three" />
                    <xs:element name="four" type="xs:string" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Sal" type="xs:unsignedShort" />
              <xs:element name="Grade">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="a" type="xs:string" />
                    <xs:element name="b" type="xs:string" />
                    <xs:element name="c" />
                    <xs:element name="d" />
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>